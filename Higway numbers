highway_number = int(input())

if highway_number <= 0 or highway_number > 999:
    print(f"{highway_number} is not a valid interstate highway number.")
elif highway_number >= 1 and highway_number <= 99: 
    if highway_number % 2 == 0:
        print(f"I-{highway_number} is primary, going east/west.")
    else:
        print(f"I-{highway_number} is primary, going north/south.")
elif highway_number >= 100 and highway_number <= 999: 
    primary_served = highway_number % 100
    if primary_served == 0 or primary_served > 99:
        print(f"{highway_number} is not a valid interstate highway number.")
    else:
        if primary_served % 2 == 0:
            direction = "east/west"
        else:
            direction = "north/south"
        print(f"I-{highway_number} is auxiliary, serving I-{primary_served}, going {direction}.")
 
